<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.oe.fiveapp"
    versionCode = "10"
    version     = "3.7.1" >
  <name>Five App</name>
  <description>Five app</description>
  <author href="https://elbaga.net" email="oelbaga@gmail.com">
      Quickreads Media LLC
  </author>
	<content src="index.html"/>
<platform name="ios" />
<platform name="android" />
<platform name="winphone" />
  <preference name="permissions" value="none"/>
  <preference name="orientation" value="default"/>
  <preference name="target-device" value="universal"/>
  <preference name="fullscreen" value="false"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="prerendered-icon" value="true"/>
  <preference name="stay-in-webview" value="false"/>
  <preference name="StatusBarOverlaysWebView" value="true" />
  <preference name="StatusBarBackgroundColor" value="#000000" />
  <preference name="StatusBarStyle" value="lightcontent" />
  <preference name="ios-statusbarstyle" value="black-opaque"/>
  <preference name="detect-data-types" value="true"/>
  <preference name="exit-on-suspend" value="false"/>
  <preference name="show-splash-screen-spinner" value="true"/>
  <preference name="auto-hide-splash-screen" value="true"/>
  <preference name="disable-cursor" value="false"/>
  <preference name="android-minSdkVersion" value="7"/>
  <preference name="android-installLocation" value="auto"/>
  <!-- <gap:plugin name="org.apache.cordova.statusbar" /> -->
  <plugin name="cordova-plugin-statusbar" />
  <!-- <gap:plugin name="org.apache.cordova.splashscreen"/>
  <gap:plugin name="org.apache.cordova.vibration"/>
  <gap:plugin name="org.apache.cordova.statusbar" /> -->  
	 <!-- http://docs.phonegap.com/phonegap-build/configuring/icons-and-splash/#splashes -->
   <!-- default android and IOS splash -->
   <splash src="splash.png" />
   <splash src="splash.png" platform="ios" width="320" height="480" />
	
			


	<!-- android icons -->
	<icon src="icons/android/48x48.png" platform="android" qualifier="ldpi" />
	<icon src="icons/android/48x48.png" platform="android" qualifier="mdpi" />
	<icon src="icons/android/72x72.png" platform="android" qualifier="hdpi" />
	<icon src="icons/android/96x96.png" platform="android" qualifier="xhdpi" />
	<icon src="icons/android/144x144.png" platform="android" qualifier="xxhdpi" />
	<icon src="icons/android/192x192.png" platform="android" qualifier="fr-xxhdpi" />
	
		<!-- ios icons -->
		<!-- iPhone 6 / 6+ -->
		<icon src="icons/ios/icon180.png" platform="ios" width="180" height="180" />
		<!-- iPhone / iPod Touch  -->
		<icon src="icons/ios/icon180.png" platform="ios" width="60" height="60" />
		<icon src="icons/ios/icon180.png" platform="ios" width="120" height="120" />
		<!-- iPad -->
		<icon src="icons/ios/icon180.png" platform="ios" width="76" height="76" />
		<icon src="icons/ios/icon180.png" platform="ios" width="152" height="152" />
		<!-- Settings Icon -->
		<icon src="icons/ios/icon180.png" platform="ios" width="29" height="29" />
		<icon src="icons/ios/icon180.png" platform="ios" width="58" height="58" />
		<!-- Spotlight Icon -->
		<icon src="icons/ios/icon180.png" platform="ios" width="40" height="40" />
		<icon src="icons/ios/icon180.png" platform="ios" width="80" height="80" />

